{
  "name": "xxxxxzenithtemplate",
  "private": true,
  "version": "0.0.0--semantic-release",
  "description": "template starter for XXXXX",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack serve --mode development --hot --progress",
    "test": "jest --clearCache && jest --coverage --passWithNoTests",
    "start-prod": "node ./server/server.js",
    "start-local-prod": "set TOP_BAR_URI=http://stagesearch.infotrack.com.au/app/proxy-assets/zenith-topmenu-homescreen&& npm run build -- --env.value=test && node ./server/server.js --localProd=true",
    "build": "webpack --mode production",
    "lint": "eslint \"*.js\" \"**/*.tsx\" \"**/*.ts\"",
    "lint-fix": "eslint \"*.js\" \"**/*.tsx\" \"**/*.ts\" --fix",
    "pretty": "pprettier --write \"**/*.{js,ts,tsx}\"",
    "clean": "rimraf ./dist",
    "analyze": "webpack --profile --json -- --mode development --env.analyze",
    "test:watch": "jest --clearCache && jest --watch --coverage",
    "version-check": "npm-check --skip-unused -u",
    "t": "jest --maxWorkers=50%",
    "tlx": "eslint \"*.js\" \"**/*.tsx\" \"**/*.ts\" && jest --maxWorkers=50% && tsc --noEmit false",
    "test-update": "jest --updateSnapshot --maxWorkers=50%",
    "test-watch": "jest --watch --maxWorkers=25%",
    "lint-staged": "npm run pretty && npm run lint-fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InfoTrackGlobal/ZenithTemplate.git"
  },
  "author": "infotrack",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/InfoTrackGlobal/ZenithTemplate/issues"
  },
  "dependencies": {
    "@infotrack/zenith-ui": "5.23.0",
    "core-js": "3.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.15.0",
    "theme-ui": "0.15.5",
    "whatwg-fetch": "3.6.17"
  },
  "homepage": "https://github.com/InfoTrackGlobal/HomeScreen#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "17.7.0",
    "@mixer/parallel-prettier": "2.0.3",
    "@testing-library/jest-dom": "6.0.0",
    "@testing-library/react": "14.0.0",
    "@types/enzyme": "3.10.13",
    "@types/jest": "26.0.15",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.4.0",
    "babel-jest": "29.6.2",
    "commitlint": "17.7.1",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.1",
    "file-loader": "6.2.0",
    "html-webpack-inject-attributes-plugin": "1.0.6",
    "html-webpack-plugin": "5.5.3",
    "husky": "^8.0.3",
    "jest": "26.6.2",
    "lint-staged": "14.0.0",
    "npm-check": "6.0.1",
    "prettier": "3.0.1",
    "react-test-renderer": "18.2.0",
    "rimraf": "5.0.1",
    "style-loader": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "ts-jest": "26.5.6",
    "ts-loader": "9.4.4",
    "typescript": "4.9.5",
    "webpack": "5.88.2",
    "webpack-bundle-analyzer": "4.9.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  }
}
